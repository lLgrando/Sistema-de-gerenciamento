<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>

<body>

    <%- include('module_panel.ejs') %>

        <div class="form_div">
            <form action="/admin/agendamento" method="post">
                <section name="cliente">
                    <div>
                        <label for="cpf">CPF</label>
                        <input type="text" id="cpf" name="cpf">
                    </div>
                    <div>
                        <label for="cliente">Cliente</label>
                        <input type="text" id="cliente" name="cliente">
                    </div>
                </section>
                <section>
                    <div>
                        <label for="servico">Serviço</label>
                        <input type="text" id="servico" name="servico">
                    </div>
                </section>
                <section name="data_e_hora" class="data_e_hora">
                    <div>
                        <label for="data">Data</label>
                        <input type="date" id="data" name="data">
                    </div>
                    <div>
                        <label for="horario">Horário</label>
                        <input type="time" id="horario" name="horario">
                    </div>
                </section>
                <section>
                    <div>
                        <label for="observacoes">Observações</label>
                        <input type="text" id="observacoes" name="observacoes">
                    </div>
                </section>
                <section class="form_button">
                    <input type="submit" value="Enviar">
                </section>
            </form>
        </div>

</body>

</html>